      "source": [
        "create_upper_triangular_matrix <- function(max_rand_int, order) {\n",
        "    result <- matrix(sample(\n",
        "        1:max_rand_int,\n",
        "        order*order,\n",
        "        replace = TRUE),\n",
        "                     ncol = order)\n",
        "\n",
        "    return (upper.tri(result, diag = TRUE) * result)\n",
        "}\n",
        "\n",
        "\n",
        "ORDER <- 9\n",
        "for (i in 1:10) {\n",
        "  A <- create_upper_triangular_matrix(max_rand_int = 10, order = ORDER)\n",
        "  B <- create_upper_triangular_matrix(max_rand_int = 20, order = ORDER)\n",
        "  C <- A %*% B\n",
        "  print(A)\n",
        "  print(B)\n",
        "  print(\"C \\n\")\n",
        "  print(C)\n",
        "\n",
        "  stopifnot(all(C == upper.tri(C, diag = TRUE) * C))\n",
        "  stopifnot(all(diag(C) == diag(A) * diag(B)))\n",
        "}\n",
        "print(\"All C's were upper triangular matrices!\")"
      ]
